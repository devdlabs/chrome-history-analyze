{
  "manifest_version": 2,

  "name": "Analyze history",
  "description": "Analyze your chrome history, analyze your life",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "history"
  ]
}